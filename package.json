{
  "name": "playlist-api",
  "version": "1.0.0",
  "description": "A TypeScript Express REST API for managing playlists, songs, artists, and albums",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/server.ts",
    "build": "tsc",
    "start": "node --env-file=.env dist/server.js",
    "db:init": "tsx --env-file=.env src/db/init.ts",
    "db:seed": "tsx --env-file=.env src/db/seed.ts"
  },
  "keywords": [
    "playlist",
    "music",
    "api",
    "rest",
    "typescript",
    "express",
    "sqlite"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
